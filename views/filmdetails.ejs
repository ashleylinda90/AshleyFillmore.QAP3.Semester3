<html lang="en">
  <%- include('./partials/header.ejs') %>
  <body>
    <div class="filmscontent">
        <div id="header">
            <img id="img1" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS0yLHIU8tFNp1CTg0XtmTYTM-gf-A9cKs-2g&usqp=CAU" alt="">
            <h1>Film Details</h1>
            <img id="img2" src="data:image/png;base64," alt="">
        </div>
      <% if (film.length > 0) { %>
        <% film.forEach(film => { %>
          <p class="film">
            <b id="b1">FILM ID:</b> <%= film.film_id %></br> 
            <b id="b1">FILM TITLE:</b> <%= film.title %></b></br>
            <b id="b1">RENTAL DATE:</b> <%= film.rental_date %>
          </p>
        <% }) %>
      <% } %>
    </div>
  </body>
  <%- include('./partials/footer.ejs') %>
</html>